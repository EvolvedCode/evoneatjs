{
  "compilerOptions": {
    "target": "es6",
    "module": "none",
    "moduleResolution": "node",
    "removeComments": true,
	  "declaration": true,
	  "outFile": "dist/evoneat.js"
  },
  "include": [
    "src/**/*",
    "typings/index.d.ts"
  ],
  "exclude": [
